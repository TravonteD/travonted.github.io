---
import Layout from "../layouts/Layout.astro";
import Books from "../components/Books.astro";

const posts = await Astro.glob("../pages/blog/*.md");
---

<Layout title="Welcome to Astro.">
  <main>
    <section>
      <h5>Blog Posts</h5>
      <ul>
        {
          posts.map((post) => (
            <li>
              <a href={post.url}>{post.frontmatter.title}</a>
            </li>
          ))
        }
      </ul>
    </section>
  </main>
</Layout>
